#include<bits/stdc++.h>
using namespace std;
int main()
{
  vector< int> a ={1,2,3,4,3,6,5,8,9,8,9,8,10};
   int n;
   cin>>n;
   for(int i=0;i<=a.size();i++){
    if(n==a[i]){
        cout<<"index number is :"<<i<<endl;
        return 0;
    }
   }
   cout<<"item is not found !"<<endl;
    return 0;
}
